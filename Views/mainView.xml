<?xml version="1.0"?>
<view icon="100" title="UtilityBelt" width="400" height="270">
  <control progid="DecalControls.Notebook" name="Notebook">
    <page label="Tools">
      <control progid="DecalControls.Notebook" name="nbMain">
        <page label="AutoSalvage">
          <control progid="DecalControls.FixedLayout" clipped="">
            <control progid="DecalControls.Checkbox" name="AutoSalvageThink" left="8" top="8" width="250" height="16" text="Think to yourself when finished"/>
            <control progid="DecalControls.Checkbox" name="AutoSalvageOnlyFromMainPack" left="8" top="32" width="250" height="16" text="Only salvage items in your main pack"/>
            <control progid="DecalControls.Checkbox" name="AutoSalvageDebug" left="8" top="56" width="250" height="16" text="Show Debug Messages"/>
            <control progid="DecalControls.PushButton" name="AutoSalvageStart" left="8" top="184" width="110" height="20" text="Start"/>

          </control>

        </page>
        <page label="AutoVendor">
          <control progid="DecalControls.FixedLayout" clipped="">
            <control progid="DecalControls.Checkbox" name="AutoVendorEnabled" left="8" top="8" width="184" height="16" text="Enable AutoVendor"/>
            <control progid="DecalControls.Checkbox" name="AutoVendorTestMode" left="8" top="32" width="208" height="16" text="Test Mode"/>
            <control progid="DecalControls.Checkbox" name="AutoVendorShowMerchantInfo" left="8" top="56" width="250" height="16" text="Show merchant info on approach"/>
            <control progid="DecalControls.Checkbox" name="AutoVendorOnlyFromMainPack" left="8" top="80" width="250" height="16" text="Only vendor items in your main pack"/>
            <control progid="DecalControls.Checkbox" name="AutoVendorThink" left="8" top="104" width="208" height="16" text="Think to yourself when finished"/>
            <control progid="DecalControls.Checkbox" name="AutoVendorDebug" left="8" top="128" width="208" height="16" text="Show Debug Messages"/>

            <control progid="DecalControls.Slider" name="AutoVendorSpeed" left="8" top="160" width="208" height="16" minimum="0" maximum="22" textcolor="0" vertical="0"/>
            <control progid="DecalControls.StaticText" name="AutoVendorSpeedLabel" left="8" top="144" width="208" height="16" justify="Center" text="Delay (ms)"/>
            <control progid="DecalControls.StaticText" name="AutoVendorSpeedText" left="456" top="24" width="40" height="16" text="0"/>
          </control>

        </page>
        <page label="InventoryManager">
          <control progid="DecalControls.FixedLayout" clipped="">
            <control progid="DecalControls.Checkbox" name="InventoryManagerAutoCram" left="8" top="8" width="400" height="16" text="Automatically cram items into side packs (while vendoring)"/>
            <control progid="DecalControls.Checkbox" name="InventoryManagerAutoStack" left="8" top="32" width="400" height="16" text="Automatically combine stacked items (while vendoring)"/>
            <control progid="DecalControls.Checkbox" name="InventoryManagerDebug" left="8" top="56" width="184" height="16" text="Show Debug Messages"/>
            <control progid="DecalControls.PushButton" name="InventoryManagerTest" left="8" top="184" width="110" height="20" text="AutoStack"/>
          </control>

        </page>
      </control>

    </page>
    <page label="DungeonMaps">
      <control progid="DecalControls.NoteBook">
        <page label="Settings">
          <control progid="DecalControls.FixedLayout" clipped="">
            <control progid="DecalControls.Checkbox" name="DungeonMapsEnabled" left="8" top="8" width="184" height="16" text="Enable Dungeon Maps"/>
            <control progid="DecalControls.Checkbox" name="DungeonMapsDrawWhenClosed" left="8" top="32" width="208" height="16" text="Draw maps when window is closed"/>
            <control progid="DecalControls.Slider" name="DungeonMapsOpacity" left="8" top="152" width="208" height="16" minimum="0" maximum="20" textcolor="0" vertical="0"/>
            <control progid="DecalControls.StaticText" name="DungeonMapsOpacityLabel" left="8" top="136" width="200" height="16" justify="Center" text="Map Opacity"/>
            <control progid="DecalControls.Checkbox" name="DungeonMapsDebug" left="8" top="56" width="208" height="16" text="Show Debug Messages"/>
            <control progid="DecalControls.Checkbox" name="DungeonMapsShowVisitedTiles" left="8" top="80" width="208" height="16" text="Show visited tiles"/>
            <control progid="DecalControls.PushButton" name="DungeonMapsClearTileCache" left="8" top="190" width="110" height="20" text="Clear Tile Cache"/>
          </control>
        </page>
        <page label="Display">
          <control progid="DecalControls.List" name="DungeonMapsSettingsList" left="0" top="0" width="400" height="200">
            <column progid="DecalControls.CheckColumn" name="DungeonMapsSettingsListEnabled"/>
            <column progid="DecalControls.IconColumn" name="DungeonMapsSettingsListColor" fixedwidth="30" />
            <column progid="DecalControls.TextColumn" name="DungeonMapsSettingsListSetting" />
          </control>
        </page>
      </control>
    </page>
    <page label="EmuConfig">
      <control progid="DecalControls.FixedLayout" clipped="">
        <control progid="DecalControls.List" name="EmuConfigList" left="0" top="0" width="490" height="250">
          <column progid="DecalControls.TextColumn" name="ConfigValue" fixedwidth="350"/>
          <column progid="DecalControls.CheckColumn" name="Enabled"/>
        </control>
      </control>

    </page>
    <page label="QuestTracker">
      <control progid="DecalControls.FixedLayout" clipped="" height="270">
        <control progid="DecalControls.StaticText" name="QuestsListFilterLabel" left="1" top="4" width="45" height="16" text="Search:"/>
        <control progid="DecalControls.Edit" name="QuestsListFilter" left="48" top="4" width="176" height="16" imageportalsrc="4726" text=""/>

        <control progid="DecalControls.Notebook" name="QuestListNotebook" left="0" top="20" width="400" height="180">
          <page label="Timed">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="17" text="Quest Flag"/>
              <control progid="DecalControls.StaticText" name="QuestsListRepeatTime" left="280" top="4" width="67" height="17" justify="Right" text="ReadyIn"/>
              <control progid="DecalControls.StaticText" name="QuestsListSolveCount" left="352" top="4" width="45" height="17" justify="Right" text="Solves"/>
              <control progid="DecalControls.List" name="TimedQuestList" left="0" top="20" width="400" height="145">
                <column progid="DecalControls.TextColumn" name="QuestKey" width="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="240"/>
                <column progid="DecalControls.TextColumn" name="RepeatTime" justify="Right" fixedwidth="100"/>
                <column progid="DecalControls.TextColumn" name="SolveCount" justify="Right" fixedwidth="60"/>
              </control>
            </control>

          </page>
          <page label="KillTask">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="24" text="Quest Flag"/>
              <control progid="DecalControls.StaticText" name="QuestsListRepeatTime" left="272" top="4" width="60" height="16" justify="Right" text="Progress"/>
              <control progid="DecalControls.StaticText" name="QuestsListSolveCount" left="336" top="4" width="60" height="16" justify="Right" text="Required"/>
              <control progid="DecalControls.List" name="KillTaskQuestList" left="0" top="20" width="400" height="145">
                <column progid="DecalControls.TextColumn" name="QuestKey" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="240"/>
                <column progid="DecalControls.TextColumn" name="SolveCount" justify="Right" fixedwidth="80"/>
                <column progid="DecalControls.TextColumn" name="MaxCompletions" justify="Right" fixedwidth="80"/>
              </control>
            </control>

          </page>
          <page label="Once">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="24" text="Quest Flag"/>
              <control progid="DecalControls.List" name="OnceQuestList" left="0" top="20" width="400" height="145">
                <column progid="DecalControls.TextColumn" name="QuestKey" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="400"/>
                <column progid="DecalControls.TextColumn" name="Unused" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="Unused2" fixedwidth="0" text="" />
              </control>
            </control>

          </page>
        </control>
        <control progid="DecalControls.PushButton" name="QuestListRefresh" left="8" top="205" width="110" height="20" text="Refresh"/>
      </control>
    </page>
    <page label="VisualNav">
      <control progid="DecalControls.FixedLayout" clipped="">
        <control progid="DecalControls.List" name="VisualNavSettingsList" left="0" top="0" width="400" height="200">
          <column progid="DecalControls.CheckColumn" name="VisualNavSettingsListEnabled"/>
          <column progid="DecalControls.IconColumn" name="VisualNavSettingsListColor" fixedwidth="30" />
          <column progid="DecalControls.TextColumn" name="VisualNavSettingsListSetting" />
        </control>
        <control progid="DecalControls.Checkbox" name="VisualNavSaveNoneRoutes" left="8" top="210" width="400" height="16" text="(Experimental) show embedded nav routes" />

      </control>

    </page>
  </control>

</view>
