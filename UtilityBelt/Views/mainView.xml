                <column progid="DecalControls.TextColumn" name="QuestKey" width="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="240"/>
                <column progid="DecalControls.TextColumn" name="RepeatTime" justify="Right" fixedwidth="100"/>
                <column progid="DecalControls.TextColumn" name="SolveCount" justify="Right" fixedwidth="60"/>
              </control>
            </control>
          </page>
          <page label="KillTask">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="24" text="Quest Flag"/>
              <control progid="DecalControls.StaticText" name="QuestsListRepeatTime" left="272" top="4" width="60" height="16" justify="Right" text="Progress"/>
              <control progid="DecalControls.StaticText" name="QuestsListSolveCount" left="336" top="4" width="60" height="16" justify="Right" text="Required"/>
              <control progid="DecalControls.List" name="KillTaskQuestList" left="0" top="20" width="400" height="145">
                <column progid="DecalControls.TextColumn" name="QuestKey" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="240"/>
                <column progid="DecalControls.TextColumn" name="SolveCount" justify="Right" fixedwidth="80"/>
                <column progid="DecalControls.TextColumn" name="MaxCompletions" justify="Right" fixedwidth="80"/>
              </control>
            </control>
          </page>
          <page label="Once">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="24" text="Quest Flag"/>
              <control progid="DecalControls.List" name="OnceQuestList" left="0" top="20" width="400" height="145">
                <column progid="DecalControls.TextColumn" name="QuestKey" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="QuestName" fixedwidth="400"/>
                <column progid="DecalControls.TextColumn" name="Unused" fixedwidth="0" text="" />
                <column progid="DecalControls.TextColumn" name="Unused2" fixedwidth="0" text="" />
              </control>
            </control>
          </page>
        </control>
        <control progid="DecalControls.PushButton" name="QuestListRefresh" left="8" top="205" width="110" height="20" text="Refresh"/>
      </control>
    </page>
    <page label="Settings">
      <control progid="DecalControls.FixedLayout" clipped="">
        <control progid="DecalControls.List" name="SettingsList" left="0" top="0" width="400" height="152">
          <column progid="DecalControls.TextColumn" fixedwidth="280" name="SettingKey" />
          <column progid="DecalControls.TextColumn" name="SettingValue" />
          <column progid="DecalControls.TextColumn" fixedwidth="3" name="_padding" />
        </control>
        <control name="SettingsForm" progid="DecalControls.FixedLayout" clipped="" left="0" top="152" width="400" height="72">
        </control>
      </control>
    </page>
  </control>
</view>
