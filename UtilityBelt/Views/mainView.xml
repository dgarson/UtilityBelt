              <column progid="DecalControls.CheckColumn" name="ChatLogTypeSelectedColumn" />
              <column progid="DecalControls.IconColumn" name="ChatLogTypeIconColumn" fixedwidth="20" />
              <column progid="DecalControls.TextColumn" name="ChatLogTypeNameColumn" />
            </control>
            <control progid="DecalControls.PushButton" name="ChatLogRuleAddButton" left="268" top="44" width="128" height="60" text="Add Rule" />
            <control progid="DecalControls.StaticText" name="ChatLogRuleListLabel" top="100" left="4" height="16" width="184" text="Rules" />
            <control progid="DecalControls.List" name="ChatLogRuleList" top="120" left="4" height="150" width="392">
              <column progid="DecalControls.IconColumn" name="ChatLogRuleRemoveColumn" />
              <column progid="DecalControls.IconColumn" name="ChatLogRuleTypeListColumn" />
              <column progid="DecalControls.TextColumn" name="ChatLogRuleMessageFilterColumn" />
            </control>
          </control>
        </page>
      </control>
    </page>
    <page label="QuestTracker">
      <control progid="DecalControls.FixedLayout" clipped="" height="270">
        <control progid="DecalControls.StaticText" name="QuestsListFilterLabel" left="1" top="4" width="45" height="16" text="Search:"/>
        <control progid="DecalControls.Edit" name="QuestsListFilter" left="48" top="4" width="176" height="16" imageportalsrc="4726" text=""/>
        <control progid="DecalControls.Notebook" name="QuestListNotebook" left="0" top="20" width="400" height="180">
          <page label="Timed">
            <control progid="DecalControls.FixedLayout" clipped="">
              <control progid="DecalControls.StaticText" name="QuestsListName" left="1" top="4" width="200" height="17" text="Quest Flag"/>
              <control progid="DecalControls.StaticText" name="QuestsListRepeatTime" left="280" top="4" width="67" height="17" justify="Right" text="ReadyIn"/>
              <control progid="DecalControls.StaticText" name="QuestsListSolveCount" left="352" top="4" width="45" height="17" justify="Right" text="Solves"/>
