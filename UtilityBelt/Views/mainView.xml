      <control progid="DecalControls.FixedLayout" clipped="">
        <control progid="DecalControls.PushButton" name="AutoVendorEnable" left="8" top="8" width="88" height="24" text="   Auto Vendor"/>
        <control progid="DecalControls.PushButton" name="AutoVendorTestMode" left="96" top="8" width="22" height="24" text="T"/>
        <control progid="DecalControls.PushButton" name="AutoTradeTestMode" left="96" top="40" width="22" height="24" text="T"/>
        <control progid="DecalControls.PushButton" name="AutoTradeEnable" left="8" top="40" width="88" height="24" text="  Auto Trade"/>
